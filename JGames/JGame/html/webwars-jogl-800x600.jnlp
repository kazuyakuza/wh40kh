<?xml version="1.0" encoding="utf-8"?>
<jnlp codebase="http://www.13thmonkey.org/~boris/jgame/JGame/html" href="webwars-jogl-800x600.jnlp">
  <information>
    <title>Web Wars</title>
    <vendor>Boris van Schooten</vendor>
    <homepage href="http://www.13thmonkey.org/~boris/jgame/"/>
    <description>A JGame example game</description>
    <offline-allowed/>
  </information>

    <resources>
      <j2se href="http://java.sun.com/products/autodl/j2se" version="1.4+"/>
      <property name="sun.java2d.noddraw" value="true"/>
      <jar href="../jars/WebWarsJogl.jar" main="true"/>
      <extension name="jogl" href="http://worldwind.arc.nasa.gov/java/jogl/webstart/jogl.jnlp" />
    </resources>

  <application-desc main-class="examples.webwars.WebWars">
  	<argument>800</argument>
  	<argument>600</argument>
  </application-desc>

</jnlp>
